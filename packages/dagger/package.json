{
  "name": "@vibe-kit/dagger",
  "version": "0.0.3",
  "type": "module",
  "description": "Local sandbox provider for Vibekit using Dagger",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:types": "tsup --dts-only",
    "dev": "tsup src/index.ts --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "vibekit",
    "local",
    "sandbox",
    "dagger",
    "development"
  ],
  "author": "Superagent-AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/superagent-ai/vibekit.git",
    "directory": "packages/dagger"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@dagger.io/dagger": "^0.18.14",
    "@octokit/rest": "^22.0.0",
    "execa": "^9.0.0",
    "fs-extra": "^11.2.0",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.18",
    "@types/which": "^3.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "peerDependencies": {
    "@vibe-kit/sdk": "*"
  }
}
