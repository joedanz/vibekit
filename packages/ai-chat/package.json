{
  "name": "@vibe-kit/ai-chat",
  "version": "0.0.1",
  "description": "AI chat package with MCP integration for VibeKit",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "ai": "^3.4.33",
    "@ai-sdk/react": "^2.0.8",
    "@ai-sdk/anthropic": "^2.0.1",
    "@vibe-kit/auth": "^0.0.5",
    "@vibe-kit/mcp-client": "^0.0.1",
    "@vibe-kit/projects": "^0.0.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.536.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.14",
    "@types/react": "^19.1.5",
    "@types/uuid": "^10.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "files": [
    "dist"
  ]
}