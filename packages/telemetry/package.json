{
  "name": "@vibe-kit/telemetry",
  "version": "0.0.1",
  "description": "Enterprise-grade telemetry system for agentic applications",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "telemetry": "./dist/cli/TelemetryCLI.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./api": {
      "types": "./dist/api/TelemetryAPIServer.d.ts",
      "import": "./dist/api/TelemetryAPIServer.js",
      "require": "./dist/api/TelemetryAPIServer.cjs"
    },
    "./plugins/*": {
      "types": "./dist/plugins/*.d.ts",
      "import": "./dist/plugins/*.js",
      "require": "./dist/plugins/*.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.201.1",
    "@vibe-kit/db": "^0.0.1",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/uuid": "^10.0.0",
    "better-sqlite3": "^11.6.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "keywords": [
    "telemetry",
    "observability",
    "agentic",
    "AI",
    "monitoring",
    "analytics"
  ],
  "author": "VibeKit Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
