{
  "name": "@vibe-kit/daytona",
  "version": "0.0.2",
  "type": "module",
  "description": "Daytona integration for VibeKit sandbox environments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch",
    "type-check": "tsc --noEmit"
  },
  "author": "Superagent-AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/superagent-ai/vibekit.git",
    "directory": "packages/daytona"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@daytonaio/sdk": "^0.22.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@vibe-kit/sdk": "*"
  }
}
